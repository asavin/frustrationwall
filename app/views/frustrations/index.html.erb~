    <% @frustrations.each do |frustration| %>
        <%= render frustration %>
    <% end %>
</div>

<script>
    $(document).scroll(function() {
        console.log($(document).scrollTop());
        if ($(document).scrollTop() > 200) {
            $('#scroll-up').fadeIn();
        }
    });
</script>

<div class="frustrations-list">
        <%= render @frustrations %>
    
    <%= will_paginate @frustrations %>
</div>

<script>
    $(document).scroll(function() {
        console.log($(document).scrollTop());
        if ($(document).scrollTop() > 200) {
            $('#scroll-up').fadeIn();
        }
        else {
            $('#scroll-up').fadeOut();
        }
    });
</script>
